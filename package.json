{
  "name": "zbet-matches-fetcher",
  "version": "1.0.0",
  "description": "Node.js application to fetch and process matches from response.json",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "view": "node index.js && node server.js",
    "view:dev": "nodemon --exec 'node index.js && node server.js'",
    "build": "node src/app.js",
    "serve": "node src/server.js",
    "serve:dev": "nodemon src/server.js",
    "app:dev": "nodemon src/app.js",
    "dev:full": "node scripts/dev.js",
    "fetch-daily": "node scripts/fetch-daily-data.js",
    "setup-cron": "node scripts/setup-cron.js",
